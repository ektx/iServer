	<header class="os-header">
		<% if (typeof title === 'undefined') { %>
		<h1>iServers</h1>
		<% } else { %>
		<h1> <a href="../<%= usr.act %>"><%= title %></a></h1>
		<% } %>

		<ul class="usr-hd-info-mod">
			<% if (usr.usr) { %>
			<li class="project-set-mod">
				<a class="add-project-set" href="#">
					<svg aria-hidden="true" height="16" version="1.1" viewBox="0 0 12 16" width="12"><path d="M12 9H7v5H5V9H0V7h5V2h2v5h5z"></path></svg>
				</a>

				<div class="usr-hd-list">
					<ul>
						<a href="">添加Git项目</a>
						<a href="/addproject">添加项目</a>
						<a href="">创建团队</a>
						<a href="">添加Demo地址</a>
					</ul>
				</div>
			</li>
			<li class="project-set-mod">

				<a >
					<span class="hd-usr-ico">
						<img src="<%= host %>/<%= usr.ico %>" alt="<%= usr.usr %>">
					</span>
					<span class="hd-usr-name"><%= usr.usr %></span>
				</a>

				<div class="usr-hd-list">
					<ul>
						<a href="/set/profile">个人中心</a>
						<a href="/loginOut">退出</a>
					</ul>
				</div>

			</li>
			<% } else { %>
			<li class="project-set-mod">
				<a href="/">
					<span class="hd-usr-name">登录</span>
				</a>
			</li>
			<li class="project-set-mod">
				<a class="signUp" href="/"><span class="hd-usr-name">注册</span></a>
			</li>
			<% } %>
		</ul>
	</header>